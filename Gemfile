source 'http://rubygems.org'

gem 'rails', '3.1.1'

group :production do
  gem 'pg'
end
group :development, :test do
  gem 'sqlite3'
end


# Gems used only for assets and not required
# in production environments by default.
group :development do
  gem 'rspec-rails'
  gem 'annotate'
end

group :test do
  gem 'rspec'
end

